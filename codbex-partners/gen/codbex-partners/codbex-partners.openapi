          application/json:
            schema:
              $ref: '#/components/schemas/CustomerFilterOptions'
            examples:
              countWithMultipleCriteria:
                summary: Count with multiple criteria
                value:
                  $filter:
                    notEquals:
                      Id: 33
                    contains:
                      FirstName: "abcd"
                      LastName: "abcd"
